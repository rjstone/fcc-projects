{
  "html": {
    "indent_size": 2,
    "indent_with_tabs": false,
    "indent_char": " ",
    "preserve_newlines": true,
    "max_preserve_newlines": 2,
    "wrap_attributes": "auto",
    "wrap_line_length": 110,
    "brace_style": "collapse",
    "indent_scripts": "normal",
    "unformatted": ["a", "sub", "sup", "b", "i", "u"]
  },
  "css": {
    "indent_char": " ",
    "indent_size": 2
  },
  "js": {
    "indent_size": 2,
    "indent_char": " ",
    "indent_level": 0,
    "indent_with_tabs": false,
    "preserve_newlines": true,
    "max_preserve_newlines": 2,
    "jslint_happy": true
  },
  "sql": {
    "indent_size": 4,
    "indent_char": " ",
    "indent_level": 0,
    "indent_with_tabs": false
  }
}
